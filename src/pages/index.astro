---
import Container from "@components/container.astro";
import Containernew from "@components/containernew.astro";
import Cta from "@components/cta.astro";
import Features from "@components/features.astro";
import Footer from "@components/footer.astro";
import Hero from "@components/hero.astro";
import Logos from "@components/logos.astro";
import Sectiondiseno from "@components/sectiondiseno.astro";
import Sectionhead from "@components/sectionhead.astro";
import Sectionhome from "@components/sectionhome.astro";
import Sectionconstruction from "@components/sectionconstruction.astro";
import Layout from "@layouts/Layout.astro";
---

<Layout title="AG">

  <Containernew bgcolor="home bg-[#E78919]">
      <Hero/>
  </Containernew>

  <Containernew bgcolor="bg-white">
    <Sectionhome/>
  </Containernew>

  <Containernew bgcolor="bg-[#003566]">
    <Sectiondiseno/>
  </Containernew>

  <Containernew bgcolor="bg-white">
    <Sectionconstruction/>
  </Containernew>

  <div class="bg-[#003566]">
    <div class="max-w-screen-lg mx-auto px-6 py-8">
      <h3 class="text-white font-bold text-4xl text-center">Nuestros Proyectos</h3>
    </div>
  </div>

  <div class="w-full grid md:grid-cols-4">
    
  </div>

  
  
  

